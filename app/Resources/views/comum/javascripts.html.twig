{# app/Resources/views/comum/javascripts.html.pt.twig #}

<script type="text/javascript">
    __loadStart = (new Date).getTime();
    var
            Preferences = "",
            User = "",
            sessionLocked = "",
            Mediboard = "",
            FALSE = false,
            TRUE = true,
            App = {
                m: "",
                a: "",
                tab: "",
                action: "",
                actionType: "",
                dialog: "",
                config: {
                    log_js_errors: ""
                },
                readonly: "",
                touchDevice: ""
            };
    Global = {
        locale: '{{ app.request.locale }}',
        assets: {
            images: {
                paciente_omissao: '{{ asset('images/identity_male.png') }}',
                calendario: '{{ asset('images/icons/calendar.gif') }}',
                btnCalendario: '{{ asset('images/icons/btn_calendar.png') }}',
                btnCalendario2: '{{ asset('images/icons/btn_calendar2.png') }}',
                tempo: '{{ asset('images/icons/time.png') }}'
            }
        },
        configs: {
            dateTimeFormat: '{{ conf_app_data_hora_mask }}',
            dateFormat: '{{ conf_app_data_mask }}',
            timeFormat: '{{ conf_app_hora_mask }}'
        }
    };
    
    Global.routes = {
        {% embed ':comum:javascripts.global.routes.html.twig' %}{% endembed %}
        minsa_url: '{{ minsa_url }}',
        ahms_url: '{{ahms_url}}'
    };
    
    Global.locales = {
        {% embed ':comum:javascripts.global.locales.html.twig' %}{% endembed %}
        __END_TAG_OBJECT: '' /* por causa do ultimo elemento do ficheiro ter , */
    };
    Global.constantes = {
        {% embed ':comum:javascripts.global.constantes.html.twig' %}{% endembed %}
        __END_TAG_OBJECT: '' /* por causa do ultimo elemento do ficheiro ter , */
    };
    
    FunctionsToCall = [];

</script>

